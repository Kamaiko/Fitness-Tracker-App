{
  "expo": {
    "name": "Halterofit",
    "slug": "halterofit",
    "version": "0.1.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "scheme": "halterofit",
    "backgroundColor": "#0A0A0A",
    "newArchEnabled": true,
    "android": {
      "package": "com.halterofit.app",
      "adaptiveIcon": {
        "backgroundColor": "#0A0A0A"
      }
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.halterofit.app"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    }
  }
}
